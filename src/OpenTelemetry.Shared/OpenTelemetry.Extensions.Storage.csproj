<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net452;net46;net461;netstandard2.0</TargetFrameworks>
    <Description>Persistent storage project for OpenTelemetry .NET</Description>
    <PackageTags>$(PackageTags);Storage</PackageTags>
  </PropertyGroup>

  <PropertyGroup>
    <NoWarn>$(NoWarn),1591</NoWarn>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\OpenTelemetry.Api\Internal\ExceptionExtensions.cs" Link="ExceptionExtensions.cs" />
  </ItemGroup>

</Project>
