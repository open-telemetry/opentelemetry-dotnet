<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net452;net46</TargetFrameworks>
    <Description>ASP.NET instrumentation for OpenTelemetry .NET</Description>
    <PackageTags>$(PackageTags);distributed-tracing;AspNet;MVC;WebAPI</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\OpenTelemetry.Api\Trace\SemanticConventions.cs" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="System.Web" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNet.TelemetryCorrelation" Version="1.0.7" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\OpenTelemetry\OpenTelemetry.csproj" />
  </ItemGroup>

</Project>
